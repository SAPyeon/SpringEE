<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.sap.mapper.ReviewMapper">
	<insert id="write">
<![CDATA[
		insert into review(id,estimate,pname,img_file,r_content,r_title)
		values(#{id},#{estimate},#{pname},#{img_file},#{r_content},#{r_title});
		]]>
	</insert>
</mapper>