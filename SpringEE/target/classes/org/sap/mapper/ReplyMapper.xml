<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.sap.mapper.ReplyMapper">
	<insert id="rewrite">
		insert reply(reply,id,bno)
		values(#{reply},#{id},#{bno});
	</insert>
	<select id="list" resultType="org.sap.model.ReplyVo">
		select * 
		from reply 
		where bno=#{bno};
	</select>
</mapper>